def main():
    print("Choose an option:")
    print("1. Say hello")
    print("2. Quit")

    choice = input("Enter your choice: ")

    if choice == "1":
        print("hello")
    elif choice == "2":
        print("Goodbye!")
    else:
        print("Invalid choice. Please try again.")

if __name__ == "__main__":
    main()


